

gem 'jek', '3.2'

# Jek plugins
group :jek_plugins do
	gem 'jek-redirect-from'
	gem 'jek-sass-converter'
	gem 'html-proofer'
	gem 'jek_github_sample'
end

# Used by Travis tests.
gem 'rake'

# Force a version lower than 5.0.0.0, which requires a newer ruby than Travis
# supports.
gem 'activesupport', '<5.0.0.0'
